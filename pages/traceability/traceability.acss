.traceability {
  width: 750rpx;
  overflow: hidden
}

.map {
  height: 680rpx;
  background: #27c7ff;
  width: 750rpx;
}

.map image {
  width: 750rpx;
  display: block
}

.map map {
  width: 750rpx;
  position: relative;
  z-index: 0;
}

.trace-wrap .title-wrap {
  width: 690rpx;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  margin: 14rpx 0 0;
  border-top-left-radius: 12rpx;
  border-top-right-radius: 12rpx;
  -webkit-border-top-left-radius: 12rpx;
  -webkit-border-top-right-radius: 12rpx;
}

.trace-wrap .title {
  font-size: 28rpx;
  width: 640rpx;
  display: inline-block;
  background: #38cdfb;
  background: -webkit-linear-gradient(top, #1adbf6, #51c1ff);
  background: -moz-linear-gradient(top, #1adbf6, #51c1ff);
  background: -o-linear-gradient(top, #1adbf6, #51c1ff);
  background: linear-gradient(top, #1adbf6, #51c1ff);
  margin: 30rpx 26rpx 10rpx;
  height: 52rpx;
  line-height: 52rpx;
  color: #fff;
  border-radius: 8rpx;
  clear: both;
}

.trace-wrap .title view {
  height: 52rpx;
  line-height: 52rpx;
}

.trace-wrap .title .white {
  display: block;
  width: 6rpx;
  height: 30rpx;
  background: #fff;
  margin-top: 10rpx;
  float: left;
}

.trace-wrap .title .text {
  line-height: 56rpx;
  padding-left: 20rpx;
  display: inline;
  background: none;
}

.trace-wrap {
  margin: 0 32rpx;
  padding-bottom: 20rpx;
  background: #fff;
  border-bottom-left-radius: 12rpx;
  border-bottom-right-radius: 12rpx;
  -webkit-border-bottom-left-radius: 12rpx;
  -webkit-border-bottom-right-radius: 12rpx;
  position: relative;
  z-index: 999;
  margin-bottom: 20rpx;
}

.trace-info {
  padding-bottom: 20rpx;
  background: url('data:image/png;base64,') 20rpx bottom no-repeat;
  background-size: 18rpx 18rpx;
}

.item {
  background: url('data:image/png;base64,') left no-repeat;
  background-size: 612.5rpx 177.7777777778rpx;
  background-position: 20rpx 10rpx;
  width: 716rpx;
  height: 158rpx;
  font-size: 28rpx;
  color: #969696;
  padding-top: 40rpx;
  padding-left: 160rpx;
  display: inline-block;
}

.trace-info .title {
  font-size: 28rpx;
  width: 640rpx;
  display: inline-block;
  background: -webkit-linear-gradient(top, #1adbf6, #51c1ff);
  margin: 30rpx 26rpx;
  height: 52rpx;
  line-height: 52rpx;
  color: #fff;
  border-radius: 8rpx;
  clear: both;
}

.trace-info .title text {
  display: block;
  width: 6rpx;
  height: 30rpx;
  background: #fff;
  margin-top: 10rpx;
  float: left;
  margin-right: 20rpx;
}

.trace-info .item>view {
  line-height: 50rpx;
}